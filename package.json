{
  "name": "nodejs-env-config",
  "version": "1.0.0",
  "description": "nodejs load env from config.json ( typescript )",
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start:test": "NODE_ENV=test node lib/index.js",
    "start:pro": "NODE_ENV=production node lib/index.js",
    "start:dev": "NODE_ENV=development node lib/index.js",
    "dev": "nodemon --watch ./src -e ts --exec \"npm run build && npm run start:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilmsg/nodejs-env-config.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ilmsg/nodejs-env-config/issues"
  },
  "homepage": "https://github.com/ilmsg/nodejs-env-config#readme",
  "devDependencies": {
    "@types/node": "^14.0.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2"
  }
}
